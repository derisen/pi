{% extends 'base.html.twig' %}

 {% block pageheader %} 
    <h1>MetadataElement</h1>
 {% endblock %} 

{% block body %}

    <div class="btn-toolbar">
    <div class="btn-group pull-right">
        <a href="{{ path('metadata_element_index') }}" class="btn btn-default">
            <span class="glyphicon glyphicon-arrow-left"></span> Back
        </a>
        <a href="{{ path('metadata_element_edit', { 'id': metadataElement.id }) }}" class="btn btn-default">
            <span class='glyphicon glyphicon-edit'></span> Edit
        </a>
        <a href="{{ path('metadata_element_delete', { 'id': metadataElement.id }) }}" class="btn btn-default" data-confirm="Are you sure you want to delete this MetadataElement?">
            <span class='glyphicon glyphicon-remove'></span> Delete
        </a>
    </div>
</div>
    <table class="table table-bordered table-condensed table-hover table-striped">
        <tbody>
            <tr>
                <th>Created</th>
                <td>{% if metadataElement.created %}{{ metadataElement.created|date }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{% if metadataElement.updated %}{{ metadataElement.updated|date }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

{% endblock %}
